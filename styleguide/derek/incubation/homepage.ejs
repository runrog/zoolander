  };
   /**
   * Helper function to ensure an image was loaded.
   */
  var ensureImageLoaded = function(url) {
    var img = new Image();
    img.onload = loadAnimation;
    img.src = url;
    if (img.complete) {
      img.onload();
    }
  };
   // If image fails to load within 3 seconds, load everything else anyway.
  timeout = setTimeout(loadAnimation, 3000);
   // Attempt to preload image.
  var url = getClassBackgroundImage('pageBanner2020-wrapper') || getClassBackgroundImage('pageBanner2020-main-image');
  ensureImageLoaded(url);
})();
</script>
